{
  "parserOptions": {
    "ecmaVersion": 2021,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },

  "settings": {
    "react": {
      "version": "detect"
    }
  },

  "plugins": [
    "react",
    "react-hooks"
  ], 

    "rules": {
  "react-hooks/rules-of-hooks": "warn",
  "react-hooks/exhaustive-deps": "warn"
}
}